{
  "name": "run.target",
  "displayName": "RunTarget",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV='development' webpack-dev-server --config webpack.config.dev.js",
    "build": "webpack --config webpack.config.prod.js --mode production --env.patch",
    "deploy": "yarn build && cordova platform add android",
    "android": "yarn build && cordova run android",
    "serve:prod": "node_modules/.bin/pm2 start process.yml",
    "serve:start": "nodemon --watch api ./api/index.js --exec babel-node --presets es2015"
  },
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
